{
  "name": "ColorCard",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "eslint",
    
    "cap:sync": "ionic cap sync",
    "cap:sync:ios": "ionic cap sync ios",
    "cap:sync:android": "ionic cap sync android",
    
    "cap:open:ios": "ionic cap open ios",
    "cap:open:android": "ionic cap open android",
    
    "cap:run:ios": "ionic cap run ios",
    "cap:run:android": "ionic cap run android",
    
    "build:web": "npm run build",
    "build:ios": "npm run build && npm run cap:sync:ios",
    "build:android": "npm run build && npm run cap:sync:android",
    "build:all": "npm run build && npm run cap:sync",
    
    "deploy:ios": "npm run build:ios && npm run cap:open:ios",
    "deploy:android": "npm run build:android && npm run cap:open:android",
    
    "cap:add:ios": "ionic cap add ios",
    "cap:add:android": "ionic cap add android",
    
    "cap:update": "ionic cap update",
    "cap:doctor": "ionic cap doctor",
    
    "clean:caps": "ionic cap sync --deployment && ionic cap update",
    "clean:build": "rm -rf dist && npm run build"
  },
  "dependencies": {
    "@babylonjs/core": "^7.41.1",
